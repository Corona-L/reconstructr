{
    "env": {
        "browser": true,
        "commonjs": true,
        "node": true,
        "es2021": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
    "indent": ["error", 2],
    "keyword-spacing": "error",
    "linebreak-style": "error",
    "quotes": ["error","single"],
    "semi": "error",
    "space-before-blocks": "error",
    "space-before-function-paren": "error"
    }
}
